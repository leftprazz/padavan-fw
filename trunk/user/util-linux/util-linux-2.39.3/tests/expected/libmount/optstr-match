false : "xxx,yyy,zzz"	:	"nozzz"
true  : "xxx,yyy,zzz"	:	"xxx,noeee"
true  : "bar,zzz"	:	"nofoo"
true  : "nofoo,bar"	:	"nofoo"
true  : "nofoo,bar"	:	"+nofoo"
false : "bar,zzz"	:	"+nofoo"
true  : "bar,zzz"	:	""
true  : "bar,zzz"	:	"+"
true  : ""	:	""
false : ""	:	"foo"
true  : ""	:	"nofoo"
false : ""	:	"no,foo"
true  : "no"	:	"+no"
